{
    "us-east-1": {
        "host_config": {
            "ami": "ami-7d0c6314",
            "instance_type": "m3.large",
            "arch": "x86_64",
            "subnet_name": "build",
            "skip_zones": ["us-east-1b"],
            "user": "ec2-user",
            "instance_name": "base-bld-linux64",
            "ssh_keyname": "aws-releng"
        },
        "distro": "centos",
        "target_virtualization_types": {
            "paravirtual": {
                "root_dev_name": "/dev/sda1",
                "boot_dev_name": "/dev/sdb",
                "aws_dev_name": "/dev/sdc",
                "int_dev_name": "/dev/xvdg",
                "fs_label": "boot_dev"
            },
            "hvm": {
                "root_dev_name": "/dev/sda1",
                "boot_dev_name": "/dev/sdb",
                "aws_dev_name": "/dev/sdd",
                "int_dev_name": "/dev/xvdh",
                "fs_label": "boot_dev"
            }
        },
        "user": "ec2-user",
        "target": {
            "size": 2,
            "fs_type": "ext4",
            "fs_label": "root_dev",
            "aws_dev_name": "/dev/sdb",
            "int_dev_name": "/dev/xvdf",
            "mount_point": "/mnt/root",
            "tags": {
                "moz-type": "bld-linux64"
            }
        }
    }
}
